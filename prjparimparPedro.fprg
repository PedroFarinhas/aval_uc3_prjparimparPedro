            <assign variable="total" expression="0"/>
            <if expression="numero1%2=0">
                <then>
                    <assign variable="resp1" expression="&quot;N&#218;MERO 1 &#201; PAR &quot; &amp; &quot;(&quot; &amp; numero1 &amp; &quot;)&quot;"/>
                </then>
                <else>
                    <assign variable="resp1" expression="&quot;N&#218;MERO 1 &#201; IMPAR &quot; &amp; &quot;(&quot; &amp; numero1 &amp; &quot;)&quot;"/>
                </else>
            </if>
            <if expression="numero2%2=0">
