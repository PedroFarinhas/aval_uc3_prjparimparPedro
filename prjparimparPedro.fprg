        <body>
            <declare name="numero1, numero2, numero3, total" type="Integer" array="False" size=""/>
            <declare name="resp1, resp2, resp3" type="String" array="False" size=""/>
            <output expression="&quot;DIGITE O PRIMEIRO N&#218;MERO&quot;" newline="True"/>
            <input variable="numero1"/>
